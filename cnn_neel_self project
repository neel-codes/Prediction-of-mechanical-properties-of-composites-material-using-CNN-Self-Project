   ],
   "source": [
    "model = Sequential()\n",
    "\n",
    "model.add(Conv2D(16, (2, 2), input_shape=(11,11,1)))\n",
    "model.add(Activation('LeakyReLU'))\n",
    "model.add(BatchNormalization())\n",
    "model.add(MaxPooling2D(pool_size=(2, 2)))\n",
    "\n",
    "model.add(Conv2D(16, (2, 2)))\n",
    "model.add(Activation('LeakyReLU'))\n",
    "model.add(BatchNormalization())\n",
    "model.add(MaxPooling2D(pool_size=(2, 2)))\n",
    "model.add(Flatten())  \n",
    "\n",
    "model.add(Dense(256,activation='LeakyReLU'))\n",
    "model.add(Dropout(0.2))\n",
    "model.add(BatchNormalization())\n",
    "model.add(Dense(128,activation='LeakyReLU'))\n",
    "model.add(Dropout(0.2))\n",
    "model.add(BatchNormalization())\n",
    "model.add(Dense(1))\n",
    "model.add(Activation('LeakyReLU'))\n",
    "model.add(BatchNormalization())\n",
    "\n",
    "model.compile(loss='mean_absolute_percentage_error', #mean_absolute_percentage_error | mean_squared_error\n",
    "              optimizer='adam',\n",
    "              )\n",
    "callbacks = [EarlyStopping(monitor='val_loss', patience=5)] # Early Stopping\n",
    "history=model.fit(xtrain, strength_train, batch_size=128, epochs=30, validation_split=0.1,callbacks=callbacks)"
   ]
  },
